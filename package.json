{
  "name": "riff-rtc",
  "description": "WebRTC browser client that connects to the riff-server",
  "version": "0.4.0-dev.1",
  "license": "MIT",
  "private": true,
  "keywords": [
    "feathers",
    "rhythm",
    "WebRTC"
  ],
  "homepage": "https://github.com/rifflearning/riff-rtc#readme",
  "author": "Dan Calacci",
  "contributors": [
    "Jordan Reedie",
    "Mike Lippert <mike@rifflearning.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rifflearning/riff-rtc.git"
  },
  "bugs": "https://github.com/rifflearning/riff-rtc/issues",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "build:prod": "coffee -c src/libs/mm.coffee; webpack --mode production --config webpack/webpack.config.js",
    "build:dev": "coffee -c src/libs/mm.coffee; webpack --mode development --config webpack/webpack.config.dev.js",
    "start:dev-server": "nodemon server.js",
    "start:dev-client": "webpack --watch --config webpack/webpack.config.js",
    "start:dev": "concurrently --kill-others \"npm run start:dev-server\" \"npm run start:dev-client\""
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/client": "^3.5.2",
    "@feathersjs/socketio-client": "^1.1.0",
    "body-parser": "^1.18.3",
    "coffeescript": "^2.3.1",
    "config": "^1.30.0",
    "cookie-parser": "^1.4.3",
    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "d3-transition": "^1.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "hogan-xpress": "^1.0.8",
    "ims-lti": "^3.0.2",
    "json-loader": "^0.5.7",
    "materialize-css": "^0.100.2",
    "react": "^16.4.0",
    "react-cookie": "^2.2.0",
    "react-dom": "^16.4.1",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "serve-static": "^1.13.2",
    "sibilant-webaudio": "^0.1.4",
    "simplewebrtc": "^3.0.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "thumos": "^1.0.0",
    "webrtc-adapter": "^6.2.1"
  },
  "devDependencies": {
    "ajv": "^6.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "hjson": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "json-strip-loader": "^1.0.2",
    "json5-loader": "^1.0.1",
    "nodemon": "^1.17.5",
    "react-dev-utils": "^5.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "3.0.0",
    "webpack-node-externals": "^1.7.2"
  }
}
