{
  "name": "rhythm-rtc",
  "description": "WebRTC browser client that connects to the rhythm-server",
  "version": "0.3.0-dev.3",
  "private": true,
  "homepage": "",
  "keywords": [
    "feathers",
    "WebRTC"
  ],
  "license": "MIT",
  "repository": {},
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "node server.js",
    "build": "node -r dotenv/config scripts/build.js",
    "dev": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "postinstall": "coffee -c ./src/libs/mm.coffee; npm run build;"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.2",
    "@feathersjs/client": "^3.5.1",
    "@feathersjs/socketio-client": "^1.1.0",
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "coffeescript": "^2.3.1",
    "connect-history-api-fallback": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "cross-spawn": "^4.0.2",
    "css-loader": "^0.28.1",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-transition": "^1.1.1",
    "dirty-json": "^0.6.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-ims-lti": "^0.2.4",
    "express-session": "^1.15.6",
